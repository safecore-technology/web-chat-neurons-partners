# Supabase Configuration
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key

# Backend API Configuration (for iframe use)
REACT_APP_API_URL=http://localhost:3001
REACT_APP_API_KEY=your-secure-api-key-for-iframe-access
REACT_APP_SOCKET_URL=http://localhost:3001

# Evolution API Configuration
REACT_APP_EVOLUTION_API_URL=http://localhost:8080
REACT_APP_EVOLUTION_API_KEY=your-evolution-api-key
REACT_APP_APP_NAME=WhatsApp Web
REACT_APP_VERSION=1.0.0

# Configurações de desenvolvimento
REACT_APP_DEBUG=true
GENERATE_SOURCEMAP=true

# Configurações de notificação
REACT_APP_NOTIFICATION_TIMEOUT=5000

# Configurações de upload
REACT_APP_MAX_FILE_SIZE=52428800
REACT_APP_ALLOWED_FILE_TYPES=image/*,video/*,audio/*,application/pdf,application/msword,application/vnd.openxmlformats-officedocument.wordprocessingml.document

# URLs de ícones e imagens
REACT_APP_DEFAULT_AVATAR=/images/default-avatar.png
REACT_APP_LOGO_URL=/images/logo.png
